<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ztiany.main"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.ztiany.base.HomeAppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/module_main_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SampleAppTheme">

        <activity android:name=".main.MainActivity">

        </activity>

        <activity
            android:name=".splash.SplashActivity"
            android:label="@string/module_main_title_activity_splash"
            android:theme="@style/SampleAppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>