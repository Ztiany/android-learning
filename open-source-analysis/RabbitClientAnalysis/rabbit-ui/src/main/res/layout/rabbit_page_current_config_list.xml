<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/rabbit_white"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:padding="5dp"
        android:text="rabbit当前配置"
        android:textColor="@color/rabbit_black"
        android:textSize="16dp"
        android:textStyle="bold" />

    <View
        android:background="@color/rabbit_divider_line"
        android:layout_width="match_parent"
        android:layout_height="1px"/>

    <com.susion.rabbit.ui.view.json.JsonRecyclerView
        android:id="@+id/mRabbitCurrentConfigPageJsonRv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="10dp" />

</LinearLayout>