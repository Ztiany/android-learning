<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mRabbitPageGlobalMonitorModeSRL"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/rabbit_white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.susion.rabbit.base.ui.view.RabbitSwitchButton
            android:id="@+id/mRabbitPageGlobalMonitorModeSwitchBtn"
            android:layout_width="match_parent"
            android:layout_height="50dp" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/mRabbitPageGlobalMonitorModeRv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </LinearLayout>


</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
